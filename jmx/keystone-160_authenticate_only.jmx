            <stringProp name="parameters"></stringProp>
            <stringProp name="script">token = vars.get(&quot;Auth_Token&quot;);
props.put(&quot;AuthToken&quot;, token);
url = vars.get(&quot;AdminURL&quot;);
var pattern = &quot;^(([^:/\\?#]+):)?(//(([^:/\\?#]*)(?::([^/\\?#]*))?))?([^\\?#]*)(\\?([^#]*))?(#(.*))?$&quot;;
var rx = new RegExp(pattern);
var parts = rx.exec(url);
OUT.println(url);
props.put(&quot;AdminHost&quot;, parts[5]);
props.put(&quot;AdminPort&quot;, parts[6]);
props.put(&quot;AdminPath&quot;, parts[7]);</stringProp>
